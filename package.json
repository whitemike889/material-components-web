    "ts-node": "^8.0.3",
    "tslint": "^5.12.0",
    "typescript": "^3.5.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-search-params-polyfill": "^7.0.0",
    "useragent": "^2.3.0",
    "verror": "^1.10.0",
    "vhtml": "^2.1.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-object-assign"
    ]
  }
}
